/home/93sam/xorriso -as mkisofs -r -checksum_algorithm_iso md5,sha1 -V 'Debian 6.0.5 i386 1' -o /org/cdbuilder.debian.org/dst/deb-cd/out/2squeezei386/debian-6.0.5-i386-NETINST-1.iso -jigdo-jigdo /org/cdbuilder.debian.org/dst/deb-cd/out/2squeezei386/debian-6.0.5-i386-NETINST-1.jigdo -jigdo-template /org/cdbuilder.debian.org/dst/deb-cd/out/2squeezei386/debian-6.0.5-i386-NETINST-1.template -jigdo-map Debian=/org/cdbuilder.debian.org/src/ftp/debian/ -jigdo-exclude boot1 -md5-list /org/cdbuilder.debian.org/src/deb-cd/tmp/2squeezei386/squeeze/md5-check -jigdo-min-file-size 1024 -jigdo-exclude 'README*' -jigdo-exclude /doc/ -jigdo-exclude /md5sum.txt -jigdo-exclude /.disk/ -jigdo-exclude /pics/ -jigdo-exclude 'Release*' -jigdo-exclude 'Packages*' -jigdo-exclude 'Sources*' -J -isohybrid-mbr syslinux/usr/lib/syslinux/isohdpfx.bin -partition_offset 16 -J -joliet-long -cache-inodes -b isolinux/isolinux.bin -c isolinux/boot.cat -no-emul-boot -boot-load-size 4 -boot-info-table boot1 CD1
